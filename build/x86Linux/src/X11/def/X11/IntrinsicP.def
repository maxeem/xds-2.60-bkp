<* IF __GEN_C__ THEN *> <*- GENTYPEDEF *> <* END *>
TYPE 
  _CorePart = RECORD
    self             : Widget;         (* pointer to widget itself *)
    widget_class     : WidgetClass;    (* pointer to Widget's ClassRec *)
    parent           : Widget;         (* parent widget *)
    xrm_name         : Xrm.XrmName;    (* widget resource name quarkified *)
    being_destroyed  : Boolean;        (* marked for destroy *)
    destroy_callbacks: XtCallbackList; (* who to call when widget destroyed*)
    constraints      : XtPointer;      (* constraint record *)
    x                : Position;
    y                : Position;       (* window position *)
    width            : Dimension;
    height           : Dimension;      (* window dimensions *)
    border_width     : Dimension;      (* window border width *)
    managed          : Boolean;        (* is widget geometry managed? *)
    sensitive        : Boolean;        (* is widget sensitive to user events *)
    ancestor_sensitive:Boolean;        (* are all ancestors sensitive? *)
    event_table      : XtEventTable;   (* private to event dispatcher *)
    tm               : XtTMRec;        (* translation management *)
    accelerators     : XtTranslations; (* accelerator translations *)
    border_pixel     : Pixel;          (* window border pixel *)
    border_pixmap    : X.Pixmap;       (* window border pixmap or NULL *)
    popup_list       : WidgetList;     (* list of popups *)
    num_popups       : Cardinal;       (* how many popups *)
    name             : String;         (* widget resource name *)
    screen           : PtrScreen;      (* window's screen *)
    colormap         : X.Colormap;     (* colormap *)
    window           : X.Window;       (* window ID *)
    depth            : Cardinal;       (* number of planes in window *)
    background_pixel : Pixel;          (* window background pixel *)
    background_pixmap: X.Pixmap;       (* window background pixmap or NULL *)
    visible          : Boolean;        (* is window mapped and not occluded? *)
    mapped_when_managed:Boolean;       (* map window if it's managed? *)
  END;
<* IF __GEN_C__ THEN *> <*+ GENTYPEDEF *> <* END *>

  CorePart = _CorePart;
  
<* IF __GEN_C__ THEN *> <*- GENTYPEDEF *> <* END *>
