    name : String;        (* resource name            *)
    type : String;        (* representation type name *)
    value: XtArgVal;      (* representation           *)
    size : INTEGER;    (* size of representation   *)
  END;

  XtTypedArgList = POINTER TO ARRAY [0..MAX_STUP_ARR] OF XtTypedArg;

  XtAllocateProc = PROCEDURE ( (* widget_class *) WidgetClass,
                               (* constraint_size *) VAR Cardinal,
                               (* more_bytes *) VAR Cardinal,
                               (* args *) ArgList,
                               (* num_args *) VAR Cardinal,
                               (* typed_args *) XtTypedArgList,
                               (* num_typed_args *) VAR Cardinal,
                               (* widget_return *) VAR Widget,
                               (* more_bytes_return *) VAR XtPointer );

  XtDeallocateProc = PROCEDURE ( (* widget *) Widget,
                                 (* more_bytes *) XtPointer );

  _XtStateRec = RECORD
    (* Declaration without definition *)
  END;

<* IF __GEN_C__ THEN *> <*- GENTYPEDEF *> <* END *>
