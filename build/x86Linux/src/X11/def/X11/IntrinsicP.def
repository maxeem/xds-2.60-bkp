TYPE 
  XtRealizeProc = PROCEDURE ( (* widget *) Widget,
                              (* mask *) VAR XtValueMask,
                              (* attributes *) VAR Xlib.XSetWindowAttributes );

  XtGeometryHandler = PROCEDURE ( (* widget *) Widget,
                                  (* request *) VAR XtWidgetGeometry,
