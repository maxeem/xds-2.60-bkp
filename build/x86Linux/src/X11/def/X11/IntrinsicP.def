(*
 * Field sizes and offsets of XrmResource must match those of XtResource.
 * Type long is used instead of XrmQuark here because XrmQuark and String
 * are not the same size on all systems.
 *)

TYPE 
  XrmResource = RECORD
    xrm_name        : LONGINT;      (*  Resource name quark		    *)
    xrm_class       : LONGINT;      (*  Resource class quark		    *)
    xrm_type        : LONGINT;      (*  Resource representation type quark  *)
    xrm_size        : Cardinal;     (*  Size in bytes of representation	    *)
    xrm_offset      : INTEGER;   (*  -offset-1			    *)
    xrm_default_type: LONGINT;      (*  Default representation type quark   *)
    xrm_default_addr: XtPointer;    (*  Default resource address	    *)
  END;

  XrmResourceList = POINTER TO ARRAY [0..MAX_STUP_ARR] OF XrmResource;

  XtVersionType = LONGCARD;

CONST 
  XT_VERSION = 11;
  XT_REVISION = 6;
  XtVersion = 11006;
  XtVersionDontCheck = 0;

TYPE
  XtProc = PROCEDURE ( );

  XtWidgetClassProc = PROCEDURE ( (* class *) WidgetClass );

  XtWidgetProc = PROCEDURE ( (* widget *) Widget );

  XtAcceptFocusProc = PROCEDURE ( (* widget *) Widget,
                                  (* time *) VAR XtValueMask ): Boolean;

  XtArgsProc = PROCEDURE ( (* widget *) Widget,
                           (* args *) ArgList,
                           (* num_args *) VAR Cardinal );

  XtInitProc = PROCEDURE ( (* request *) Widget,
                           (* new *) Widget,
                           (* args *) ArgList,
                           (* num_args *) VAR Cardinal );

  XtSetValuesFunc = PROCEDURE ( (* old *) Widget,
                                (* request *) Widget,
                                (* new *) Widget,
                                (* args *) ArgList,
                                (* num_args *) VAR Cardinal ): Boolean;

  XtArgsFunc = PROCEDURE ( (* widget *) Widget,
                           (* args *) ArgList,
                           (* num_args *) VAR Cardinal ): Boolean;

  XtAlmostProc = PROCEDURE ( (* old *) Widget,
                             (* new *) Widget,
                             (* request *) VAR XtWidgetGeometry,
                             (* reply *) VAR XtWidgetGeometry );

  XtExposeProc = PROCEDURE ( (* widget *) Widget,
                             (* event *) Xlib.PtrXEvent,
                             (* region *) Xutil.Region );

CONST
(*  compress_exposure options *)
  XtExposeNoCompress       = {};
  XtExposeCompressSeries   = {0};
  XtExposeCompressMultiple = {1};
  XtExposeCompressMaximal  = {0,1};

(*  modifiers  *)
  XtExposeGraphicsExpose       = {4};
  XtExposeGraphicsExposeMerged = {5};
  XtExposeNoExpose             = {6};
