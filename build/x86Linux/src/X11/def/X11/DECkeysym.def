(*
 * DEC private keysyms
 * (29th bit set)
 *)

(*  two-key compose sequence initiators, chosen to map to Latin1 characters  *)

CONST 
  DXK_ring_accent = 1000FEB0H;
  DXK_circumflex_accent = 1000FE5EH;
  DXK_cedilla_accent = 1000FE2CH;
  DXK_acute_accent = 1000FE27H;
  DXK_grave_accent = 1000FE60H;
  DXK_tilde = 1000FE7EH;
  DXK_diaeresis = 1000FE22H;

(*  special keysym for LK2** "Remove" key on editing keypad  *)
  DXK_Remove = 1000FF00H;              (*  Remove  *)

END DECkeysym.
